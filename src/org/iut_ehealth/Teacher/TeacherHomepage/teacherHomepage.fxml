<AnchorPane id="teacherHomepage" prefHeight="600.0" prefWidth="1000.0" stylesheets="@../../styles/styles.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.iut_ehealth.Teacher.TeacherHomepage.teacherHomepageController">
    <children>
        <Pane layoutY="-1.0" prefHeight="600.0" prefWidth="246.0" style="-fx-background-color: black;">
            <children>
                <JFXButton id="navButton" fx:id="editprofilebutton" layoutX="57.0" layoutY="192.0" onAction="#onEditProfileClick" prefHeight="40.0" prefWidth="116.0" style="-fx-border-color: white; -fx-background-radius: 20;" text="Edit Profile" textFill="WHITE" />
                <JFXButton id="navButton" fx:id="seeyellowslipsButton" layoutX="57.0" layoutY="251.0" onAction="#onSeeYellowSlipsClick" prefHeight="40.0" prefWidth="116.0" text="See Yellow Slips" textFill="WHITE" />
                <JFXButton fx:id="uploadImage" layoutX="58.0" layoutY="134.0" onAction="#uploadImageHandler" style="-fx-background-color: white;" text="Upload Image" />
                <ImageView fx:id="profilePicture" fitHeight="100.0" fitWidth="100.0" layoutX="69.0" layoutY="20.0" pickOnBounds="true" preserveRatio="true" />
                <JFXButton fx:id="browseButton" layoutX="156.0" layoutY="134.0" onAction="#browseHandler" prefHeight="25.0" prefWidth="23.0" style="-fx-background-color: white;" text="..." />
              <JFXButton fx:id="logoutButton" layoutX="83.0" layoutY="319.0" onAction="#onLogoutButtonClick" prefHeight="47.0" prefWidth="64.0" text="Logout" />
            </children>
        </Pane>
      <Label layoutX="382.0" layoutY="263.0" prefHeight="47.0" prefWidth="488.0" text="Welcome to IUT E-Health System">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
