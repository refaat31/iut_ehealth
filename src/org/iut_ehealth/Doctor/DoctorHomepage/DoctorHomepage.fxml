<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="doctorHomepage" prefHeight="600.0" prefWidth="1000.0" stylesheets="@../../styles/styles.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.iut_ehealth.Doctor.DoctorHomepage.DoctorHomepageController">
    <children>
        <Pane layoutY="-1.0" prefHeight="600.0" prefWidth="246.0" style="-fx-background-color: black;">
            <children>
                <JFXButton id="navButton" fx:id="editProfileButton" layoutX="57.0" layoutY="184.0" onAction="#onEditProfileClick" prefHeight="40.0" prefWidth="116.0" text="Edit Profile" textFill="WHITE" />
                <JFXButton id="navButton" fx:id="seeAppointmentButton" layoutX="57.0" layoutY="241.0" onAction="#onSeeAppointmentClick" prefHeight="40.0" prefWidth="116.0" text="See Appointments" textFill="WHITE" />
                <JFXButton id="navButton" fx:id="BillRequests" layoutX="54.0" layoutY="300.0" onAction="#onBillRequestsClick" prefHeight="40.0" prefWidth="116.0" text="Bill Requests" textFill="WHITE" />
                <JFXButton id="navButton" layoutX="53.0" layoutY="359.0" onAction="#onSlipRequestsClick" prefHeight="40.0" prefWidth="116.0" text="Slip Requests" textFill="WHITE" />
                <JFXButton fx:id="uploadImage" layoutX="58.0" layoutY="134.0" onAction="#uploadImageHandler" style="-fx-background-color: white;" text="Upload Image" />
                <ImageView fx:id="profilePicture" fitHeight="100.0" fitWidth="100.0" layoutX="69.0" layoutY="20.0" pickOnBounds="true" preserveRatio="true" />
                <JFXButton fx:id="browseButton" layoutX="156.0" layoutY="134.0" onAction="#browseHandler" prefHeight="25.0" prefWidth="23.0" style="-fx-background-color: white;" text="..." />
              <JFXButton fx:id="logoutButton" layoutX="82.0" layoutY="429.0" onAction="#onLogoutButtonClick" prefHeight="40.0" prefWidth="60.0" text="Logout" />
            </children>
        </Pane>
      <Label layoutX="382.0" layoutY="263.0" prefHeight="47.0" prefWidth="488.0" text="Welcome to IUT E-Health System">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
