<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="studentTeacherConnection" prefHeight="600.0" prefWidth="1000.0" stylesheets="@../../styles/styles.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.iut_ehealth.Student.StudentTeacherConnection.studentTeacherConnectionController">
    <children>
        <Label layoutX="290.0" layoutY="24.0" prefHeight="47.0" prefWidth="384.0" text="Connect With a Teacher">
         <font>
            <Font name="System Bold" size="30.0" />
         </font></Label>
        <Pane layoutY="-1.0" prefHeight="600.0" prefWidth="246.0" style="-fx-background-color: black;">
            <children>
                <JFXButton id="navButton" fx:id="editProfileButton" layoutX="57.0" layoutY="181.0" onAction="#onEditProfileClick" prefHeight="40.0" prefWidth="116.0" text="Edit Profile" textFill="WHITE" />
                <JFXButton id="navButton" fx:id="myAppointmentsButton" layoutX="57.0" layoutY="232.0" onAction="#onMyAppointmentsClick" prefHeight="40.0" prefWidth="116.0" text="My Appointments" textFill="WHITE" />
                <JFXButton id="navButton" fx:id="StudentRefundsButton" layoutX="54.0" layoutY="287.0" onAction="#onStudentRefundsClick" prefHeight="40.0" prefWidth="116.0" text="Refunds" textFill="WHITE" />
                <JFXButton id="navButton" fx:id="PrescriptionButton" layoutX="52.0" layoutY="339.0" onAction="#onPrescriptionButtonClick" prefHeight="40.0" prefWidth="116.0" text="Prescriptions" textFill="WHITE" />
                <JFXButton id="navButton" fx:id="MedicalRecordsButton" layoutX="52.0" layoutY="399.0" onAction="#onStudentMedicalRecordsClick" prefHeight="40.0" prefWidth="116.0" text="Medical Records" textFill="WHITE" />
                <JFXButton fx:id="uploadImage" layoutX="68.0" layoutY="143.0" onAction="#uploadImageHandler" style="-fx-background-color: white;" text="Upload Image" />
                <JFXButton id="navButtonCurrentPage" fx:id="StudentTeacherConnectionButton" layoutX="32.0" layoutY="453.0" onAction="#onStudentTeacherConnectionClick" prefHeight="40.0" prefWidth="156.0" text="Connect with a Teacher" textFill="WHITE" />
                <ImageView fx:id="profilePicture" fitHeight="100.0" fitWidth="100.0" layoutX="69.0" layoutY="29.0" pickOnBounds="true" preserveRatio="true" />
                <JFXButton fx:id="browseButton" layoutX="166.0" layoutY="143.0" onAction="#browseHandler" prefHeight="25.0" prefWidth="23.0" style="-fx-background-color: white;" text="..." />
            <JFXButton id="navButton" fx:id="zoombutton" layoutX="46.0" layoutY="513.0" onAction="#onZoomClick" prefHeight="40.0" prefWidth="58.0" text="zoom" textFill="WHITE" />
              <JFXButton fx:id="logoutButton" layoutX="121.0" layoutY="513.0" onAction="#onLogoutButtonClick" prefHeight="40.0" prefWidth="58.0" text="Logout" />
            </children>
        </Pane>
      <Label layoutX="305.0" layoutY="178.0" prefHeight="47.0" prefWidth="313.0" text="Enter a Teacher ID to connect with the teacher :">
         <font>
            <Font size="14.0" />
         </font></Label>
      <JFXTextField fx:id="teacherIDField" layoutX="618.0" layoutY="186.0" prefHeight="32.0" prefWidth="135.0" promptText="Teacher ID" />
      <JFXButton id="acceptButton" fx:id="ConnectButton" layoutX="695.0" layoutY="276.0" onAction="#onConnectClick" prefHeight="32.0" prefWidth="116.0" text="Connect">
         <font>
            <Font size="14.0" />
         </font></JFXButton>
    </children>
</AnchorPane>
