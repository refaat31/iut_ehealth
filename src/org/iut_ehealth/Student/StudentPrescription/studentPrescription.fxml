        <JFXTreeTableView fx:id="prescriptionListView" layoutX="280.0" layoutY="363.0" onKeyPressed="#getSelectedItemKey" onMouseClicked="#getSelectedItem" prefHeight="180.0" prefWidth="694.0" stylesheets="@../../styles/tableView.css" />
