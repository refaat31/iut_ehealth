<AnchorPane id="studentMakeAppointment" prefHeight="600.0" prefWidth="1000.0" stylesheets="@../../styles/styles.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.iut_ehealth.Student.StudentsAppointments.studentMakeAppointmentController">
    <children>
        <Label layoutX="436.0" layoutY="31.0" prefHeight="47.0" prefWidth="369.0" text="Make an Appointment">
         <font>
            <Font name="System Bold" size="30.0" />
         </font></Label>
        <Pane layoutY="-1.0" prefHeight="600.0" prefWidth="246.0" style="-fx-background-color: black;">
            <children>
                <JFXButton id="navButton" fx:id="editProfileButton" layoutX="57.0" layoutY="181.0" onAction="#onEditProfileClick" prefHeight="40.0" prefWidth="116.0" text="Edit Profile" textFill="WHITE" />
                <JFXButton id="navButtonCurrentPage" fx:id="myAppointmentsButton" layoutX="57.0" layoutY="232.0" onAction="#onMyAppointmentsClick" prefHeight="40.0" prefWidth="116.0" text="My Appointments" textFill="WHITE" />
                <JFXButton id="navButton" fx:id="StudentRefundsButton" layoutX="54.0" layoutY="287.0" onAction="#onRefundsClick" prefHeight="40.0" prefWidth="116.0" text="Refunds" textFill="WHITE" />
                <JFXButton id="navButton" fx:id="PrescriptionButton" layoutX="52.0" layoutY="339.0" onAction="#onPrescriptionButtonClick" prefHeight="40.0" prefWidth="116.0" text="Prescriptions" textFill="WHITE" />
                <JFXButton id="navButton" fx:id="MedicalRecordsButton" layoutX="52.0" layoutY="399.0" onAction="#onStudentMedicalRecordsClick" prefHeight="40.0" prefWidth="116.0" text="Medical Records" textFill="WHITE" />
                <JFXButton fx:id="uploadImage" layoutX="68.0" layoutY="143.0" onAction="#uploadImageHandler" style="-fx-background-color: white;" text="Upload Image" />
                <JFXButton id="navButton" fx:id="StudentTeacherConnectionButton" layoutX="32.0" layoutY="453.0" onAction="#onStudentTeacherConnectionClick" prefHeight="40.0" prefWidth="156.0" text="Connect with a Teacher" textFill="WHITE" />
                <ImageView fx:id="profilePicture" fitHeight="100.0" fitWidth="100.0" layoutX="69.0" layoutY="29.0" pickOnBounds="true" preserveRatio="true" />
                <JFXButton fx:id="browseButton" layoutX="166.0" layoutY="143.0" onAction="#browseHandler" prefHeight="25.0" prefWidth="23.0" style="-fx-background-color: white;" text="..." />
              <JFXButton fx:id="logoutButton" layoutX="121.0" layoutY="513.0" onAction="#onLogoutButtonClick" prefHeight="40.0" prefWidth="58.0" text="Logout" />
            </children>
        </Pane>
      <Label layoutX="331.0" layoutY="149.0" prefHeight="34.0" prefWidth="83.0" text="Problem" />
      <JFXTextField fx:id="problemField" layoutX="431.0" layoutY="153.0" prefHeight="25.0" prefWidth="468.0" />
      <JFXComboBox fx:id="dayField" layoutX="431.0" layoutY="230.0" promptText="Day" />
      <Label layoutX="264.0" layoutY="225.0" prefHeight="34.0" prefWidth="134.0" text="Date of Appointment" />
      <JFXComboBox fx:id="monthField" layoutX="518.0" layoutY="230.0" prefHeight="25.0" prefWidth="63.0" promptText="Month" />
      <Label layoutX="350.0" layoutY="300.0" prefHeight="34.0" prefWidth="45.0" text="Time" />
      <JFXComboBox fx:id="timeField" layoutX="428.0" layoutY="305.0" prefHeight="25.0" prefWidth="83.0" />
      <JFXButton id="acceptButton" fx:id="MakeAnAppointment" layoutX="518.0" layoutY="382.0" onAction="#onMakeAnAppointmentClick" prefHeight="34.0" prefWidth="156.0" text="Make Appointment" />
      <JFXButton id="navzoom" fx:id="zoombutton" layoutX="46.0" layoutY="513.0" onAction="#onZoomClick" prefHeight="40.0" prefWidth="58.0" text="zoom" textFill="WHITE">
